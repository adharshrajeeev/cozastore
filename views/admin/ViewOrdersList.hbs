<body>
    <br>
  
    <br>
    <table class="table">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">User</th>
      <th scope="col">Date</th>
      <th class="col">Payment Method</th>
      <th scope="col">Price</th>
      {{!-- <th class="col">Status</th> --}}
      <th class="col">Product Wise</th>
     
    </tr>
  </thead>
  <tbody>
    {{#each ordersList}}
    <tr>
      <th scope="row">{{inc @index}}</th>
      <td>{{this.address.name}}</td>
      <td>{{this.date}}</td>
      <th>{{this.paymentMethod}}</th>
       <td>{{this.totalAmount}}</td>
       {{!-- <td>{{this.status}}</td> --}}
      <td>
        <a href="/admin/adminproductOrders/{{this._id}}" class="btn btn-primary">View Products</a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>
</body>
